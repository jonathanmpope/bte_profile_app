<% if current_user %>
    <h2>Welcome, <%=current_user.email%>!</h2>
    <h2><%= link_to "Logout", '/logout', method: :delete %></h2>
<% end %>
<%= form_with url: '/profile', method: :get, local: true do |form|%>
    <h2>Assessment Track</h2>
    <%= form.select :track, [["SOF Maritime Contract",'sof_mar_cont'],["SOF Land Contract",'sof_land_cont'],['SOF Maritime Prep','sof_mar_prep'], ['SOF Land Prep','sof_land_prep']], required: true %>
    <br>
    <br>
    <%= form.submit "Select Track" %>
<% end %> 

<% if @track != nil && @track == 'sof_land_cont' %>
    <h2>SOF Land Contract Assessments</h2>
<% end %>